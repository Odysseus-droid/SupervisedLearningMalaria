drive/MyDrive/FINAL PROJECT DATA SCI/V2 Main Malaria Cases.csv', names=column_names, header=0)\n","\n","# Filter for Afghanistan\n","afghanistan_data = data[data[\"Location\"] == \"Afghanistan\"]"],"metadata":{"id":"yjI8FUlgCcC5"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["train_data = afghanistan_data[afghanistan_data[\"Period\"].between(2000, 2020)]\n","\n","test_data = afghanistan_data[afghanistan_data[\"Period\"] == (2021)]"],"metadata":{"id":"W2Mi5Rkyna27"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# Define features and target\n","features = [\"Population\", \"Estimated number of malaria cases\", \"Mean Max temp\", \"Mean Min temp\", \"Mean Precipitation\", \"Median Max temp\",\n","            \"Median Min temp\",\"Median Precipitation\",\"Sum Max temp\",\"Sum Min temp\",\"Sum Precipitation\", \"Malaria Case Rate\", \"Temperature Range\"]\n","\n","target = \"Estimated number of malaria cases\"\n","\n","# Training set\n","X_train = train_data[features]\n","y_train = train_data[target]\n","\n","# Testing set\n","X_test = test_data[features]\n","y_test = test_data[target]  # This is for comparison after prediction"],"metadata":{"id":"WsHIwAiOqaHP"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["from sklearn.ensemble import GradientBoostingRegressor\n","from sklearn.metrics import mean_absolute_error, mean_squared_error, r2_score\n","\n","# Initialize the model\n","model = GradientBoostingRegressor(random_state=42)\n","\n","# Train the model\n","model.fit(X_train, y_train)\n","\n","# Make predictions for 2006\n","y_pred = model.predict(X_test)\n","\n","# Evaluation (excluding R-squared due to single sample)\n","mae = mean_absolute_error(y_test, y_pred)\n","rmse = mean_squared_error(y_test, y_pred, squared=False)\n","r2 = r2_score(y_test, y_pred)\n","\n","print(\"Performance Metrics:\")\n","print(f\"MAE: {mae}\")\n","print(f\"RMSE: {rmse}\")\n","print(f\"R2: {r2}\")\n","\n","# Compare actual vs. predicted\n","print(\"Actual:\", y_test.values)\n","print(\"Predicted:\", y_pred)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"PqCq1Oywqd66","executionInfo":{"status":"ok","timestamp":1733494014896,"user_tz":-480,"elapsed":286,"user":{"displayName":"JOHN ODYSSEUS LIM","userId":"03132053339468839938"}},"outputId":"36d2b042-8120-4418-c2a5-07e606966ed9"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Performance Metrics:\n","MAE: 79362.31939266989\n","RMSE: 79362.31939266989\n","R2: nan\n","Actual: [193232]\n","Predicted: [272594.31939267]\n"]}]},{"cell_type":"code","source":["import warnings\n","import numpy as np\n","import pandas as pd\n","from sklearn.model_selection import TimeSeriesSplit\n","from sklearn.ensemble import GradientBoostingRegressor\n","from sklearn.metrics import mean_absolute_error, r2_score\n","\n","# Load your dataset\n","data = pd.read_csv('/content/drive/MyDrive/FINAL PROJECT DATA SCI/V2 Main Malaria Cases.csv')\n","\n","# Replace zeros with ones globally to avoid issues during log transformation\n","data = data.replace(0, 1)\n","\n","# Ensure data is sorted for time series split\n","data = data.sort_values(by=['Location', 'Period'])\n","\n","# Define countries\n","countries = data['Location'].unique()\n","\n","# Suppress warnings\n","warnings.filterwarnings(\"ignore\", category=FutureWarning, message=\"'squared' is deprecated\")\n","\n","# Initialize model\n","gb_model = GradientBoostingRegressor(n_estimators=100, random_state=42)\n","\n","# Initialize TimeSeriesSplit\n","n_splits = 10  # Adjust as needed\n","tscv = TimeSeriesSplit(n_splits=n_splits)\n","\n","# Epsilon for log transformation\n","epsilon = 1\n","\n","# Define log transformation\n","def log_transformation(x):\n","    return np.log(x + epsilon)\n","\n","# Initialize metric storage\n","all_mae_scores = []\n","all_mape_scores = []\n","all_r2_scores = []  # Add R^2 storage\n","error_logs = []\n","\n","# Loop through countries\n","for country in countries:\n","    print(f\"\\nProcessing country: {country}\")\n","    country_mae_scores = []\n","    country_mape_scores = []\n","    country_r2_scores = []  # Add R^2 scores for each country\n","    country_errors = []\n","\n","    try:\n","        # Filter data\n","        country_data = data[data['Location'] == country]\n","\n","        # Define features and target\n","        X = country_data[[\"Population\", \"Mean Max temp\", \"Mean Min temp\", \"Mean Precipitation\",\n","                          \"Median Max temp\", \"Median Min temp\", \"Median Precipitation\",\n","                          \"Sum Max temp\", \"Sum Min temp\", \"Sum Precipitation\",\n","                          \"Malaria Case Rate\", \"Temperature Average\"]]\n","        y = country_data['Estimated number of malaria cases']\n","\n","        # Added condition to skip countries with no. of rows < n_splits\n","        if len(X) < n_splits:\n","            raise ValueError(f\"Not enough data points ({len(X)}) for {n_splits}-fold time series split in {country}.\")\n","\n","        # Shift negative values in temperature columns (before applying log transformation)\n","        temperature_columns = [\"Mean Min temp\", \"Median Min temp\", \"Sum Min temp\"]\n","        for column in temperature_columns:\n","            if data[column].min() < 0:\n","                shift_value = abs(data[column].min()) + 1\n","                print(f\"Shifting {column} by {shift_value} to handle negative values.\")\n","                data[column] += shift_value\n","\n","        # TimeSeriesSplit cross-validation\n","        for train_index, test_index in tscv.split(X):\n","            X_train, X_test = X.iloc[train_index], X.iloc[test_index]\n","            y_train, y_test = y.iloc[train_index], y.iloc[test_index]\n","\n","            # Apply log transformation\n","            X_train_transformed = log_transformation(X_train)\n","            X_test_transformed = log_transformation(X_test)\n","            y_train_transformed = log_transformation(y_train)\n","            y_test_transformed = log_transformation(y_test)\n","\n","            # Train and predict using transformed data\n","            gb_model.fit(X_train_transformed, y_train_transformed)\n","            y_pred = gb_model.predict(X_test_transformed)\n","\n","            # Calculate metrics\n","            mae = mean_absolute_error(y_test_transformed, y_pred)\n","            mape = np.mean(np.abs((y_test_transformed - y_pred) / (y_test_transformed + epsilon))) * 100\n","            r2 = r2_score(y_test_transformed, y_pred)  # Calculate R^2 score\n","\n","            # Store metrics\n","            country_mae_scores.append(mae)\n","            country_mape_scores.append(mape)\n","            country_r2_scores.append(r2)  # Store R^2 score\n","\n","        # Store average metrics for the country\n","        avg_mae = np.mean(country_mae_scores)\n","        avg_mape = np.mean(country_mape_scores)\n","        avg_r2 = np.mean(country_r2_scores)  # Calculate average R^2 for the country\n","\n","        print(f\"  Average MAE: {avg_mae:.2f}\")\n","        print(f\"  Average MAPE: {avg_mape:.2f}%\")\n","        print(f\"  Average R^2: {avg_r2:.2f}\")  # Print average R^2 for the country\n","\n","        all_mae_scores.append(avg_mae)\n","        all_mape_scores.append(avg_mape)\n","        all_r2_scores.append(avg_r2)  # Store average R^2 score for all countries\n","\n","    except Exception as e:\n","        country_errors.append(f\"Error processing {country}: {e}\")\n","        print(f\"Error processing {country}: {e}\")\n","\n","    error_logs.append(country_errors)\n","\n","# Summary of overall average MAE, MAPE, and R^2\n","print(\"\\nSummary of Overall Results:\")\n","\n","# Calculate the overall average MAE, MAPE, and R^2\n","if all_mae_scores:  # Check if there are any valid results\n","    overall_avg_mae = np.mean(all_mae_scores)\n","    overall_avg_mape = np.mean(all_mape_scores)\n","   